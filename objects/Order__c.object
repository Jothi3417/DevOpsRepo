    <validationRules>
        <fullName>GST</fullName>
        <active>true</active>
        <errorConditionFormula>AND(  Total_Price__c &gt;2000, ISNULL( GST_number__c ))</errorConditionFormula>
        <errorDisplayField>GST_number__c</errorDisplayField>
        <errorMessage>Your  price exceeds Rs. 2000 So you need to populate GST number</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Order_Date</fullName>
        <active>true</active>
        <errorConditionFormula>Order_Date__c &lt;  TODAY()</errorConditionFormula>
        <errorDisplayField>Order_Date__c</errorDisplayField>
        <errorMessage>Order date can’t be prior to current date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Order_status</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISCHANGED(  Order_Status__c ),
OR(
ISPICKVAL(PRIORVALUE(  Order_Status__c  ),&quot;Delivered&quot;)
)
)</errorConditionFormula>
        <errorDisplayField>Order_Status__c</errorDisplayField>
        <errorMessage>The status of order can’t be changed to any other status once it set to be delivered.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Phone</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(OR(REGEX( Phone__c , &quot;^[0-9]{10}&quot;)))</errorConditionFormula>
        <errorDisplayField>Phone__c</errorDisplayField>
        <errorMessage>The phone field must contain number and that too of 10 digits</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
